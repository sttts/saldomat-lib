install-srcdoc: $(PACKAGE).tag
	test -d "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)" || \
	  mkdir -p "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)"
	for f in apidoc/*; do \
	  cp $$f "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)/"; \
	done
	cp "$(PACKAGE).tag" "$(DESTDIR)/Users/michael/apidoc/"
	if test -x "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)/installdox"; then \
	  cd "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)" && \
	   ID_OPT="-q" ; \
	   for ff in ${DOXYGEN_TAGFILES}; do \
	    ID_OPT="$$ID_OPT -l `basename $$ff`@../`basename $$ff | $(SED) -e s/.tag//`";\
	   done ;\
	   ( cd "$(DESTDIR)/Users/michael/apidoc/$(PACKAGE)" && \
	      ./installdox -q $$ID_OPT \
	   ); \
	fi

rpm:    $(PACKAGE).spec dist
	rpmbuild="rpm" && \
	if [ `rpm --version | awk '{ print $$3 }'` > /dev/null ]; then rpmbuild="rpmbuild"; fi && \
	$$rpmbuild -ta $(PACKAGE)-$(VERSION).tar.gz
